#include <stdio.h>

int main() {
    int a = 5;
    int b = 10;
    int c = 3;

    int temp = a + b;

    int result1 = temp * c;
    int result2 = temp - c; 
    int result3 = temp + c; 

    printf("Result 1: %d\n", result1);
    printf("Result 2: %d\n", result2);
    printf("Result 3: %d\n", result3);

    return 0;
}
